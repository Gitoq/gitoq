{
  "name": "envlock",
  "version": "0.0.0",
  "description": "A secrets manager for .env files â€“ from the same people that pioneered dotenv.",
  "author": "envlock",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/dotenv-wallet/dotenv-cli",
  "bugs": "https://github.com/Desktop/envlock/issues",
  "repository": "dotenv-wallet/dotenv-cli",
  "license": "MIT",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "envlock": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "orval": "orval",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "pnpm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "pnpm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "pnpm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "envlock",
    "commands": "./dist/commands",
    "dirname": "envlock",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "warn-if-update-available": {
      "timeoutInDays": 1
    }
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5.0.14",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.2.11",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5.1.3",
    "eslint-config-oclif-typescript": "^3.1.6",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.8.8",
    "orval": "^6.27.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
