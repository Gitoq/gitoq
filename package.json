{
  "name": "dotenv-wallet",
  "version": "0.0.0",
  "description": "A secrets manager for .env files â€“ from the same people that pioneered dotenv.",
  "author": "dotenv-wallet",
  "bin": {
    "dotenv": "./bin/run.js",
    "dotenv-wallet": "./bin/run.js"
  },
  "homepage": "https://github.com/dotenv-wallet/dotenv-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "dotenv-wallet/dotenv-cli",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3.0.12",
    "@oclif/plugin-warn-if-update-available": "^3.0.12"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.4.12",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "binAliases": [
      "dotenv",
      "dotenv-wallet"
    ],
    "dirname": "dotenv-wallet",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    },
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "bugs": "https://github.com/Soheiljafarnejad/dotenv-cli/issues",
  "keywords": [
    "dotenv",
    "dotenv-wallet",
    "env",
    ".env",
    "environment",
    "variables"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
